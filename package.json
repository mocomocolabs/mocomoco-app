{
  "name": "mocomoco-app",
  "version": "0.0.1",
  "private": true,
  "description": "mocomoco-app",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run front\"",
    "front": "ionic serve -p 3000",
    "server": "nodemon --watch server server/index.js",
    "build": "react-scripts build",
    "serve:build": "npx serve -s build",
    "sync": "yarn build && npx cap sync",
    "dev:ios": "ionic capacitor run ios -l --external",
    "dev:and": "ionic capacitor run android -l --external",
    "open:and": "npx cap open android",
    "open:ios": "npx cap open ios",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx,js,jsx}'",
    "typecheck": "tsc --noEmit --incremental false",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@capacitor/cli": "^3.0.1",
    "@types/crypto-js": "^4.0.1",
    "@types/lodash": "^4.14.170",
    "@types/mobx-task": "^2.0.1",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router": "^5.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-svg": "^5.0.0",
    "@types/sockjs-client": "^1.5.0",
    "@types/stompjs": "^2.3.4",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-preset-jest": "^27.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "morgan": "^1.10.0",
    "prettier": "^2.3.1",
    "type-fest": "^1.2.1"
  },
  "dependencies": {
    "@capacitor/android": "^3.0.1",
    "@capacitor/core": "^3.0.1",
    "@capacitor/ios": "^3.0.1",
    "@capacitor/storage": "^1.0.1",
    "@ionic/react": "^5.6.9",
    "@ionic/react-router": "^5.6.9",
    "@stomp/stompjs": "^6.1.0",
    "axios": "^0.21.1",
    "compressorjs": "^1.0.7",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.10.5",
    "inko": "^1.1.1",
    "ionicons": "^5.5.2",
    "lodash": "^4.17.21",
    "mobx": "^5.15.7",
    "mobx-react-lite": "^2.2.2",
    "mobx-task": "^2.0.1",
    "node-sass": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.2",
    "react-hook-form": "^7.9.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-svg": "11.2.5",
    "sockjs-client": "^1.5.1",
    "typescript": "^4.3.4"
  },
  "eslintConfig": {
    "extends": "react-app, prettier"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
